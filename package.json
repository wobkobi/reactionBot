  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node build/index.js",
    "dev": "nodemon --watch src --ext ts --exec \"node --loader ts-node/esm/transpile-only src/index.ts\"",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobkobi/reactionBot.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/wobkobi/reactionBot/issues"
  },
  "homepage": "https://github.com/wobkobi/reactionBot#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^52.0.2",
    "eslint-plugin-prettier": "^5.5.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.1",
    "prettier-plugin-organize-imports": "^4.2.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "overrides": {
    "undici": "7.10.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,css,scss,html,json,yml,yaml,md,mdx}": "prettier --write"
